<?xml version="1.0" encoding="UTF-8"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd"
	xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.4.xsd">
	<params>
		<param name="stream.filter.type">DOM</param>
		<param name="inputType">input.xml</param>
	</params>

	<jb:bean beanId="personnelLeave" retain="true"
		class="com.transgrid.mib.ellipse.hr.domain.EllipseEmployeeLeave"
		createOnElement="DataArea/PersonnelLeave">
		<jb:value data="EmployeeID" property="empId" />
		<jb:value data="LeaveType" property="leaveType" />
		<jb:value data="LeaveReason" property="leaveReason" />
		<jb:value data="LeaveApprovalStatus" property="leaveApprovalStatus" />
		
		<jb:value data="LeaveStartDate" property="leaveStartDate" />
		<jb:value data="LeaveStartTime" property="leaveStartTime" />
				
		<jb:value data="LeaveEndDate" property="leaveEndDate" />
		<jb:value data="LeaveEndTime" property="leaveEndTime" />
		
		<jb:value data="RequestCreatedDate" property="reqCreatedDate" />
		<jb:value data="RequestCreatedTime" property="reqCreatedTime" />
		
		<jb:value data="RequestCreatedBy" property="reqCreatedBy" /> 
		
		<jb:value data="RequestApprovedDate" property="reqApprovedDate" />
		<jb:value data="RequestApprovedTime" property="reqApprovedTime" />
		
		<jb:value data="RequestApprovedBy" property="reqApprovedBy" />
	</jb:bean> 

</smooks-resource-list>

